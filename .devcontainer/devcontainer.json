{
  "name": "py-launch-blueprint",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "postCreateCommand": "just --version",
  "mounts": [
    "source=/var/run/docker.sock,target=/var/run/docker.sock,type=bind"
  ],
  "remoteUser": "root",
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  }
}
